    "test": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "1.17.1",
    "@types/jest": "26.0.7",
    "@types/puppeteer": "1.20.3",
    "jest": "26.1.0",
    "jest-cli": "26.1.0",
    "puppeteer": "1.20.0"
  },
  "license": "MIT"
}
