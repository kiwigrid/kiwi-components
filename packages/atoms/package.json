{
  "name": "@kiwigrid/kiwi-atoms",
  "description": "Common Components for Kiwigrid Apps",
  "version": "0.3.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/atoms/atoms.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "clean": "rimraf dist loader www .stencil",
    "lint": "eslint --fix '*/**/*.{ts,tsx}'",
    "start": "run-p start:stencil start:storybook",
    "start:stencil": "stencil build --docs --dev --watch --serve --port 3334",
    "start:storybook": "start-storybook -p 6007 -s www",
    "test": "NODE_ICU_DATA=node_modules/full-icu stencil test --spec",
    "test:watch": "NODE_ICU_DATA=node_modules/full-icu stencil test --spec --watchAll",
    "generate": "stencil generate",
    "build": "run-s build:stencil-ci build:storybook",
    "build:stencil-ci": "stencil build --ci",
    "build:stencil": "stencil build --docs",
    "build:storybook": "build-storybook"
  },
  "dependencies": {
    "@popperjs/core": "2.5.4",
    "date-fns": "^2.16.1",
    "i18next": "^19.8.3",
    "i18next-xhr-backend": "^3.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@stencil/core": "^2.2.0",
    "@stencil/eslint-plugin": "0.3.1",
    "@stencil/store": "1.3.0",
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/web-components": "^6.0.28",
    "@types/events": "^3.0.0",
    "@types/jest": "26.0.15",
    "@types/lodash-es": "4.17.3",
    "@types/puppeteer": "5.4.0",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "babel-loader": "^8.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "full-icu": "1.3.1",
    "jest": "26.6.2",
    "jest-cli": "26.6.2",
    "jest-date-mock": "1.0.8",
    "lit-html": "^1.3.0",
    "lodash-es": "4.17.15",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "puppeteer": "5.4.1",
    "rimraf": "3.0.2",
    "rxjs-marbles": "^6.0.1",
    "typescript": "4.0.5"
  },
